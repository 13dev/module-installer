{
    "name": "13dev/module-installer",
    "description": "A Module Installer",
    "keywords": [
        "php",
        "laravel",
        "laravel-modules",
        "modules"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "13dev",
            "email": "leonardooliveira.tc24@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.6.*|5.7.*",
        "nwidart/laravel-modules": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "autoload": {
       "psr-4": {
            "Dev13\\ModuleInstaller\\": "src/ModuleInstaller"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dev13\\ModuleInstaller\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dev13\\ModuleInstaller\\ModuleInstallerServiceProvider"
            ]
        }
    },
    "license": "MIT"
    
}
